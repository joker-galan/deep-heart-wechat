/* custom-tab-bar/index.wxss */
.item{
    width: 100%;
    height: 140rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    background-color: white;
}

.item-left {
    display: flex;
    width: 100rpx;
    height: 60rpx;
    background-image: url("https://blogx.oss-cn-hangzhou.aliyuncs.com/wechat/deep-heart/bar_left.png");
    background-repeat: no-repeat;
    background-size: 100% 100%;
    line-height: 60rpx;
    margin-top: 10rpx;
}

.item-right {
    display: flex;
    width: 100rpx;
    height: 60rpx;
    background-image: url("https://blogx.oss-cn-hangzhou.aliyuncs.com/wechat/deep-heart/bar_right.png");
    background-repeat: no-repeat;
    background-size: 100% 100%;
    line-height: 60rpx;
    margin-top: 10rpx;
}